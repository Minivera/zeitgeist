{
  "name": "manifold-integration",
  "alias": "manifold-integration.zeit.sh",
  "version": 2,
  "builds": [
    { "src": "src/index.ts", "use": "@now/node" },
    { "src": "src/oauth.ts", "use": "@now/node" }
  ],
  "routes": [
    { "src": "/", "dest": "src/index.ts" },
    { "src": "/login/oauth", "dest": "src/oauth.ts" }
  ],
  "env": {
    "NODE_ENV": "development"
  }
}
