{
  "name": "manifold-integration",
  "alias": "manifold-integration.zeit.sh",
  "version": 2,
  "routes": [
    { "src": "/", "dest": "src/index.ts" },
    { "src": "/static/(.*)", "dest": "static/$1" },
    { "src": "/(.*)", "dest": "src/endpoints/$1.ts" }
  ],
  "builds": [{ "src": "./src/index.ts", "use": "@now/node@canary" }]
}
